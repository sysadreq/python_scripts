   "source": [
    "# show uid, for each file in the current directory\n",
    "for filenames in os.listdir(os.curdir):\n",
    "        res=os.stat(filenames)\n",
    "        uid=res[5]\n",
    "        size=res[7]\n",
    "        print(filenames, uid, size)\n",
    "    "
   ]
